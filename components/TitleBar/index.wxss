/* components/TitleBar/index.wxss */

.icon-wrap-component {
  height: 88rpx;
  width: 40%;
  justify-content: left;
  align-items: center;
  display: flex;
  padding-bottom: 12rpx;
}
.icon-wrap-component .icon-item {
  width: 36rpx;
  height: 36rpx;
  margin-left: 20rpx;
  font-weight: 400;
  color: black;
}
.back-arrow {
  color: #ffffff;
  font-size: 36rpx;
  margin-left: 48rpx;
}
.title-placeholder {
  width: 48rpx;
  min-width: 48rpx;
  height: 48rpx;
}
.icon-title {
  color: #ffffff;
  font-size: 36rpx;
  font-weight: 600;
  line-height: 40rpx;
  text-align: center;
  margin-left: 12rpx;
  white-space: nowrap;
}
.icon-notify-title {
  color: #696969;
  font-size: 48rpx;
  padding-bottom: 12rpx;
  margin-left: 56rpx;
}
